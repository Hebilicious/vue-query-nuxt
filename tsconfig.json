{
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "strict": true,
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "target": "esnext",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "#imports": ["packages/vue-query-nuxt/node_modules/nuxt/app.d.ts"],
      "#app": ["packages/vue-query-nuxt/node_modules/nuxt/dist/app"],
      "#build/internal.vue-query-plugin-callback": ["packages/vue-query-nuxt/src/runtime/internal"]
    }
  },
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/playground",
    "**/playgrounds",
    "**/docs"
  ]
}
