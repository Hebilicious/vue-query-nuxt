{
  "name": "@hebilicious/vue-query-nuxt",
  "type": "module",
  "version": "0.2.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "playgrounds/*",
    "test",
    "docs"
  ],
  "scripts": {
    "build": "rimraf packages/*/dist && cd packages/vue-query-nuxt && bun run build",
    "lint": "eslint --cache .",
    "lint:fix": "bun lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "pnpm lint:fix && bumpp -r -x \"pnpm run changelog\" --all",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepare:types": "cd packages/vue-query-nuxt && nuxi prepare",
    "publish:ci": "pnpm -r publish --access public --no-git-checks"
  },
  "devDependencies": {
    "@hebilicious/eslint-config": "0.0.3-beta.3",
    "@nuxt/test-utils": "^3.7.3",
    "@types/node": "^20.6.2",
    "bumpp": "^9.2.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "lint-staged": "^14.0.1",
    "pnpm": "8.6.12",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "simple-git-hooks": {
    "pre-commit": "bunx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
